<h1>Genre Name:  <%=@genre.name%></h1>

<h2>Genre's Artists:</h2>
<ul>
	<li><%@genre.artists.each do |artist|%></li>
  	<li><a href="/artists/<%=artist.slug %>"><%=artist.name%></a></li>
  	<li><%end%></li>
</ul>

<h2>Genre's Songs:</h2>
<ul>
	<li><%@genre.songs.each do |song|%></li>
	<li><a href="/songs/<%=song.slug %>"><%=song.name%></a></li>
	<li><%end%></li>
</ul>
