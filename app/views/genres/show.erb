<h1>Genre Name:  <%=@genre.name%></h1>

<h2>Genre's Artists:</h2>

	<p><%@genre.artists.each do |artist|%></p>
  	<p><a href="/artists/<%=artist.slug %>"><%=artist.name%></a></p>
  	<p><%end%></p>

<h2>Genre's Songs:</h2>

	<p><%@genre.songs.each do |song|%></p>
	<p><a href="/songs/<%=song.slug %>"><%=song.name%></a></p>
	<p><%end%></p>
