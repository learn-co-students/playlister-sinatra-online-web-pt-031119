<h1>Create a New Song:</h1>
<form action="/songs" method="POST">
  <h3>Song Name: <input type="text" name="Name" placeholder="Song Name:"></h3>
  <h3>Genres:</h3>
  <%@genres.each do |genre|%>
	<input type="checkbox" name="genres[]" id="<%= genre.name%>" value="<%=genre.id%>"><%=genre.name%></input>
</form>


<input type="checkbox" name="owner[pet_ids][]" id="<%= pet.id%>" value="<%=pet.id%>" <%='checked' if @owner.pets.include?(pet) %>><%=pet.name%></input>