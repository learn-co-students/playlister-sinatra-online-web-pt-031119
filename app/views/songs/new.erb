<h1>Create a New Song:</h1>
<form action="/songs" method="POST">	
  <h3>Song Name: <input type="text" name="Name" id="Name" placeholder="Song Name:"></h3>
  <h3>Genres:</h3>
  <%@genres.each do |genre|%>
   	<input type="checkbox" name="genres[]" id="<%=genre.name%>" value="<%=genre.id%>"><%=genre.name%></input>
  <%end%>
	<h3>Artist Name: <input type="text" name="Artist Name" id="Artist Name" placeholder="Artist Name:"></h3>
  <input type="submit" value="Create">
 </form>