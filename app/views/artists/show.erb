<h1>Artist Name:  <%=@artist.name%></h1>

<h2>Artist's Songs:</h2>
<ul>
	<li><%@artist.songs.each do |song|%></li>
  	<li><a href="/songs/<%=song.slug %>"><%=song.name%></a></li>
  	<li><%end%></li>
</ul>

<h2>Artist's Genres:</h2>
<ul>
	<li><%@artist.genres.each do |genre|%></li>
	<li><a href="/genres/<%=genre.slug %>"><%=genre.name%></a></li>
	<li><%end%></li>
</ul>
